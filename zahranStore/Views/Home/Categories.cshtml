 @model List<Category>

<section id="services" class="services section-bg">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>اختار نوع المنتج</h2>
          <p>تسوق من منزلك من خلال موقعنا</p>
        </div>

        <div class="row gy-4">
            @foreach(var item in Model)
            {
              <div  class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="zoom-in" data-aos-delay="100">
                <div style="width: 430px;" class="icon-box iconbox-blue">
                  <a href="/Home/Products/@item.Id"><img style="border-radius:20%;border:7px solid #5e8ea0;
                      width:400px;height:400px  "
                      src="../@item.Photo" /></a>
                  <h4><a href="/Home/Products/@item.Id">@item.Name</a></h4>
                  <p><a href="/Home/Products/@item.Id">@item.Description</a></p>
                </div>
               </div>
           }
        </div>
      </div>
    </section><!-- End Services Section -->